<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textview_dia"
        android:layout_width="330dp"
        android:layout_height="63dp"
        android:layout_marginBottom="367dp"
        android:layout_marginEnd="19dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="81dp"
        android:gravity="center"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button_dia"
        android:layout_width="99dp"
        android:layout_height="64dp"
        android:layout_marginBottom="249dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="54dp"
        android:background="@drawable/round_button1"
        android:gravity="center"
        android:onClick="onclickdialog"
        android:text="Copy"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_dia"
        app:layout_constraintVertical_bias="0.0" />

</android.support.constraint.ConstraintLayout>